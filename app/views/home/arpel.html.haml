                    %img{"data-description" => "Remodelación, apartamentos Arpel", "data-title" => "Araña", :src => "assets/proyectos-thumbs/Arpel02.jpg"}
                  %a{:href => "assets/proyectos/Arpel03.jpg"}
                    %img{"data-title" => "Remodelación, apartamentos Arpel", :src => "assets/proyectos-thumbs/Arpel03.jpg"}
